{{!
    @template local_sandbox/index_page

    Starting point for an awesome template.

}}
<div class="container-fluid" data-region="index-page">
    Show all moodle users.
    {{#users}}
    <div class="row">
        <div class="col-xs-2">{{id}}</div>
        <div class="col-xs-2">{{username}}</div>
        <div class="col-xs-2">{{email}}</div>
        <div class="col-xs-2">{{firstname}}</div>
        <div class="col-xs-2">{{lastname}}</div>
    </div>
    {{/users}}
</div>